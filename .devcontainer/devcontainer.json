{
    "name": "${localWorkspaceFolderBasename}",
    "service": "gateway",
    "workspaceFolder": "/home/gateway",
    "mounts": [
        {
            "source": "${localWorkspaceFolder}",
            "target": "/home/gateway",
            "type": "bind"
        },
        {
            "source": "${localEnv:HOME}/.ssh",
            "target": "/root/.ssh:ro",
            "type": "bind"
        }
    ],
    "runServices": [
        "gateway"
    ],
    "dockerComposeFile": "compose.yaml",
    "initializeCommand": "awk '/^import.*\"@solana\\/spl-token\".*/{if(prev!=\"// @ts-ignore\"){print\"// @ts-ignore\"}}{prev=$0;print}' ${localWorkspaceFolder}/src/chains/solana/solana.ts > /tmp/solana.ts && mv /tmp/solana.ts ${localWorkspaceFolder}/src/chains/solana/solana.ts",
    "onCreateCommand": "apt-get update && apt-get install -y git",
    "overrideCommand": true,
    "customizations": {
        "vscode": {
            "extensions": [
                "stylelint.vscode-stylelint",
                "esbenp.prettier-vscode",
                "dbaeumer.vscode-eslint",
                "redhat.vscode-yaml",
                "tamasfe.even-better-toml",
                "DavidAnson.vscode-markdownlint",
                "shd101wyy.markdown-preview-enhanced",
                "semanticdiff.semanticdiff",
                "sourcegraph.cody-ai",
                "mhutchie.git-graph"
            ]
        }
    }
}